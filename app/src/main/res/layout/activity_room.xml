<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/teal_700">

    <zjy.android.bliveinteract.widget.WarGameView
        android:id="@+id/war_game"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/wei_warrior"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:padding="10dp"
        android:text="weiWarrior"
        app:layout_constraintLeft_toRightOf="@id/war_game"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/shu_warrior"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@color/white"
        android:padding="10dp"
        android:text="shuWarrior"
        app:layout_constraintLeft_toRightOf="@id/war_game"
        app:layout_constraintTop_toBottomOf="@id/wei_warrior" />

    <TextView
        android:id="@+id/wu_warrior"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@color/white"
        android:padding="10dp"
        android:text="wuWarrior"
        app:layout_constraintLeft_toRightOf="@id/war_game"
        app:layout_constraintTop_toBottomOf="@id/shu_warrior" />

    <TextView
        android:id="@+id/qun_warrior"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@color/white"
        android:padding="10dp"
        android:text="qunWarrior"
        app:layout_constraintLeft_toRightOf="@id/war_game"
        app:layout_constraintTop_toBottomOf="@id/wu_warrior" />

    <TextView
        android:id="@+id/wei_speed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:padding="10dp"
        android:text="weiSpeed"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/shu_speed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@color/white"
        android:padding="10dp"
        android:text="shuSpeed"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/wei_speed" />

    <TextView
        android:id="@+id/wu_speed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@color/white"
        android:padding="10dp"
        android:text="wuSpeed"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/shu_speed" />

    <TextView
        android:id="@+id/qun_speed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@color/white"
        android:padding="10dp"
        android:text="qunSpeed"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/wu_speed" />

    <TextView
        android:id="@+id/collect"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@id/war_game"/>

</androidx.constraintlayout.widget.ConstraintLayout>